 Model: Fichiers=Fichier Features=Feature Cibles=Cible Standardizers=Standardizer Modeles=Modele;
    Fichier: 'USE' name=ID 
             sparator=Separator?
    ;
    Separator: 'SEPARATOR' name=TypeSeparator;

    TypeSeparator: ","|";"|"/";

    Feature: 'FEATURE' names+=Colonne[','];

    Colonne: name=ID;

    Modele: 'MODEL' name= Type;

    Type: "linear" | "svm" | "logistic";

    Cible: 'TARGET' name=ID;

    Standardizer: 'STD' state=BOOL;

    Comment: /\/\/.*/;